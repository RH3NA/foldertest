<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="405.0" prefWidth="384.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guitest.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0">
     <content>
       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
     </content>
   </ScrollPane>
   <ButtonBar prefHeight="40.0" prefWidth="200.0">
     <buttons>
       <Button fx:id="sokButton" mnemonicParsing="false" onAction="#onSokButtonClick" text="Sök" />
     </buttons>
   </ButtonBar>
   <ButtonBar prefHeight="40.0" prefWidth="200.0">
     <buttons>
       <Button fx:id="lanButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onLanButtonClick" text="Lån" />
     </buttons>
   </ButtonBar>
    <Button fx:id="exitButton" alignment="BOTTOM_CENTER" onAction="#onExitButtonClick" text="Avsluta" />
   <TextArea fx:id="resultText" prefHeight="87.0" prefWidth="344.0" />
</VBox>
